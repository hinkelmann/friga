{% extends 'base.admin.html.twig' %}
{% block titulo %} {{ frigaedital.uuid }}/Resultado{% endblock %}
{% block breadcumbsTitulo %} {{ frigaedital.uuid }}/Resultado {% endblock %}
{% block conteudo %}
    {{ form_start(config_form) }}
    <div class="row">
        {{ include('@NteAplicacaoFriga/edital/menu.html.twig', {entidade: frigaedital}) }}
        <div class="col-md-9 col-lg-10">
            <section class="panel-body">
                <div class="row">
                    <div class="col-md-6">
                        <div class="row">
                            <div class="form-group">
                                <label class="col-md-4 col-sm-4 control-label">
                                    Resultados
                                </label>
                                <div class="col-md-8">
                                    {{ form_widget(config_form) }}
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                    </div>
                </div>
                <div class="col-md-12 text-center">
                    <button class="btn btn-primary " type="submit">
                        <i class="fa fa-save"></i> Salvar
                    </button>
                </div>
            </section>
        </div>
    </div>
    {{ form_end(config_form) }}
{% endblock %}