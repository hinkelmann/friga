    <script src="{{ asset('assets/vendor/modernizr/modernizr.js') }}"></script>
</head>
<body>
{% block superCorpo %}
    <section class="body">
        {% block cabecalho %}
            {% include '::topo.admin.html.twig' %}
        {% endblock %}
        <div class="inner-wrapper">
            {% block menu1 %}
                {% include '::menu.admin.html.twig' %}
            {% endblock %}
            <section role="main" class="content-body">
                {% block breadcumbs %}
                    <header class="page-header hidden-print">
                        <h2>{% block breadcumbsTitulo %}Inicio{% endblock %}</h2>
                        <div class="right-wrapper pull-right">
                            <ol class="breadcrumbs">
                                <li><a href="{{ path('nte_admin_painel_homepage') }}"
                                       title="Clique aqui para ir para o painel de controle"><i class="fa fa-home"></i></a>
                                </li>
                                {% block breadcumbsItens %}
                                    <li><span></span></li>{% endblock %}
                            </ol>
                            <a class="sidebar-right-toggle" data-open="sidebar-right"
                               title="Clique aqui para visualizar os usuÃ¡rios do sistema"><i
                                        class="fa fa-chevron-left"></i></a>
                        </div>
                    </header>
                {% endblock %}

                {% if app.request.hasPreviousSession %}
                    {% for type, messages in app.session.flashbag.all() %}
                        {% for message in messages %}
                            <div class="alert alert-{{ type }} fade in">
                                <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                                <strong><i class="fa fa-info-circle"></i></strong>
                                {{ message }}
                            </div>
                        {% endfor %}
                    {% endfor %}
                {% endif %}
                {% block conteudo %}


                {% endblock %}
            </section>
            {% block menu2 %}
                {% include '::menu.admin2.html.twig' %}
            {% endblock %}
        </div>
        {% block rodape %}

        {% endblock %}
    </section>
{% block javascripts %}
